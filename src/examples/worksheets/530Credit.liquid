{% block script %}
  <script defer>
    document.addEventListener("DOMContentLoaded", function () {
      // Use querySelector with escaped ID: \\5 is how you escape 5
      const select = document.querySelector("[id='530select']");
      const modalTrigger = document.getElementById("modal-trigger");

      function updateView() {
        const selected = select.value; // Get the selected option's value

        // Remove "active" class from all tables and modals
        document.querySelectorAll(".table, .modal").forEach(el => {
          el.classList.remove("active");
        });

        // Add "active" class to the selected table and modal
        document.getElementById(`table-${selected}`).classList.add("active");
        document.getElementById(`modal-${selected}`).classList.add("active");

        modalTrigger.setAttribute("data-target", `#modal-${selected}`);
      }

      // Set default view
      updateView();

      // Listen for changes in dropdown selection
      select.addEventListener("change", updateView);
    });
  </script>

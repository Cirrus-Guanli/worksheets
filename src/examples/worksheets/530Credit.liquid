<table class="table table-sm table-bordered table-hover worksheet-table mb-0">
  <caption class="sr-only">
    {{
      title
    }}
  </caption>
  <thead>
    <tr>
      <th scope="col" class="bg-bg-blue align-middle text-center">Actions</th>
      <th scope="col" class="bg-bg-blue align-middle text-center">Bldg #</th>
      <th scope="col" class="bg-bg-blue align-middle">Address</th>
      <th scope="col" class="bg-bg-blue align-middle text-center">FMA Grant?</th>
      <th scope="col" class="bg-bg-blue align-middle text-center">FPP<br>(Post-Project)</th>
      <th scope="col" class="bg-bg-blue align-middle text-center">FPB<br>(Pre-Project)</th>
      <th scope="col" class="bg-bg-blue align-middle text-center">FPI<br>(FPP - FBP)</th>
      <th scope="col" class="bg-bg-blue align-middle text-center">TU<br>(Technique Used)</th>
      <th scope="col" class="bg-bg-blue align-middle text-center">PB<br>(TU x FPI)</th>
      <th scope="col" class="bg-bg-blue align-middle">Building Type</th>
      <th scope="col" class="bg-bg-blue align-middle text-center"># of Buildings</th>
      <th scope="col" class="bg-bg-blue align-middle text-center">No. of Buildings w/ FMA Multiplier and TU</th>
      <th scope="col" class="bg-bg-blue align-middle text-center">PB w/ FMA Multiplier</th>
    </tr>
  </thead>
  <tbody>
    {% for item in fpcccitems %}
    <tr>
      <td class="text-center align-middle text-text-ph"><span class="align-middle material-symbols-outlined">delete</span><span data-toggle="modal"
    data-target="#exampleModal2" class="ml-2 align-middle material-symbols-outlined">edit</span></td>
      <th class="text-center align-middle" scope="row">OP#{{ forloop.index }}</th>
      <td class="text-nowrap align-middle">{{ item.address }}</td>
      <td class="text-center align-middle">{{ item.fma_grant? }}</td>
      <td class="text-center align-middle">{{ item.fpp }}</td>
      <td class="text-center align-middle">{{ item.fpb }}</td>
      <td class="text-center align-middle">{{ item.fpi }}</td>
      <td class="text-center align-middle">{{ item.tu }}</td>
      <td class="text-center align-middle">{{ item.pb }}</td>
      <td class="align-middle">{{ item.building_type }}</td>
      <td class="text-center align-middle">{{ item.num_of_buildings }}</td>
      <td class="text-center align-middle">{{ item.num_buildings_fma_and_tu }}</td>
      <td class="text-center align-middle">{{ item.pb_fma_multiplier }}</td>
    </tr>
    {% endfor %}
  </tbody>
  <tfoot>
    <tr>
      <td scope="row" colspan="12" class="text-right text-purple mr-3"><strong>Option 1:</strong><span class="d-block text-muted">c530 = 2.4 x Total # of Buildings that qualify x Technique Used (TU)</span></td>
      <td scope="row" class="text-center text-purple align-middle font-weight-bold">0</td>
    </tr>
    <tr>
      <td scope="row" colspan="12" class="text-right text-purple mr-3"><strong>Option 2:</strong><span class="d-block text-muted">c530 = 16 x SPB x 100 / bSF</span></td>
      <td scope="row" class="text-center text-purple align-middle font-weight-bold">0</td>
    </tr>
  </tfoot>
</table>

{% block modal %}
  <!-- Modal -->
  <div
    class="modal fade"
    id="exampleModal"
    tabindex="-1"
    aria-labelledby="exampleModalLabel"
    aria-hidden="true"
  >
    <div class="modal-dialog modal-dialog-scrollable">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Add Building</h5>
          <button
            type="button"
            class="close"
            data-dismiss="modal"
            aria-label="Close"
          >
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend>Address</legend>
            <label for="formGroupExampleInput" class="sr-only"
              >Address</label
            >
            <input
              type="text"
              class="browser-default form-control"
              id="formGroupExampleInput"
              placeholder="Enter the address"
            />
          </fieldset>
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend>FMA Grant?</legend>
            <label for="formGroupExampleInput2" class="sr-only"
              >FMA Grant?</label
            >
            <select class="custom-select">
              <option selected value="N">No</option>
              <option value="Y">Yes</option>
            </select>
          </fieldset>
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend>FPP (Post-Project)</legend>
            <label for="formGroupExampleInput" class="sr-only"
              >FPP (Post-Project)</label
            >
            <input
              type="text"
              inputmode="numeric"
              class="browser-default form-control"
              id="formGroupExampleInput"
              placeholder="Enter a number"
            />
          </fieldset>
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend>FPB (Pre-Project)</legend>
            <label for="formGroupExampleInput2" class="sr-only">FPB (Pre-Project)</label>
            <input
              type="text"
              inputmode="numeric"
              class="browser-default form-control"
              id="formGroupExampleInput2"
              placeholder="Enter a number"
            />
          </fieldset>
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend>FPI (FPP - FBP)</legend>
            <label for="formGroupExampleInput2" class="sr-only">FPI (FPP - FBP)</label>
            <input
              type="text"
              inputmode="numeric"
              class="browser-default form-control"
              id="formGroupExampleInput2"
              placeholder="Enter a number"
            />
          </fieldset>
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend>TU (Technique Used)</legend>
            <label for="formGroupExampleInput2" class="sr-only">TU (Technique Used)</label>
            <input
              type="text"
              inputmode="numeric"
              class="browser-default form-control"
              id="formGroupExampleInput2"
              placeholder="Enter a number"
            />
          </fieldset>
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend>PB (TU x FPI)</legend>
            <label for="formGroupExampleInput2" class="sr-only">PB (TU x FPI)</label>
            <input
              type="text"
              inputmode="numeric"
              class="browser-default form-control"
              id="formGroupExampleInput2"
              placeholder="Enter a number"
            />
          </fieldset>
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend>Building Type</legend>
            <div class="form-group">
              <label for="formGroupExampleInput2" class="sr-only">Building Type</label>
              <select class="custom-select">
                <option selected value="Regular building in the SFHA">Regular building in the SFHA</option>
                <option selected value="Repetitive Loss building in the SFHA">Repetitive Loss building in the SFHA</option>
                <option selected value="Repetitive Loss building in the X Zone">Repetitive Loss building in the X Zone</option>
                <option selected value="Severe Repetitive Loss building in the SFHA">Severe Repetitive Loss building in the SFHA
                </option>
                <option selected value="Severe Repetitive Loss building in the X Zone">Severe Repetitive Loss building in the X Zone
                </option>
                <option selected value="Critical Facility protected to the 500yr flood level">Critical Facility protected to the
                  500yr flood level</option>
                <option selected value="Other Critical Facility">Other Critical Facility</option>
              </select>
            </div>
          </fieldset>
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend># of Buildings</legend>
            <label for="formGroupExampleInput2" class="sr-only"># of Buildings</label>
            <input
              type="text"
              inputmode="numeric"
              class="browser-default form-control"
              id="formGroupExampleInput2"
              placeholder="Enter a number"
            />
          </fieldset>
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend># of Buildings w/ FMA Multiplier & TU</legend>
            <label for="formGroupExampleInput2" class="sr-only"># of Buildings w/ FMA Multiplier & TU</label>
            <input type="text" inputmode="numeric" class="browser-default form-control" id="formGroupExampleInput2"
              placeholder="Enter a number" />
          </fieldset>
          <fieldset class="mb-2 px-3 pb-3 pt-1">
            <legend>PB w/ FMA Multiplier</legend>
            <label for="formGroupExampleInput2" class="sr-only">PB w/ FMA Multiplier</label>
            <input type="text" inputmode="numeric" class="browser-default form-control" id="formGroupExampleInput2"
              placeholder="Enter a number" />
          </fieldset>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-link" data-dismiss="modal">Cancel</button>
          <button type="button" class="btn btn-purple" data-dismiss="modal">
            Add
          </button>
        </div>
      </div>
    </div>
  </div>
{% endblock %}

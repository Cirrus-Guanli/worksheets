              class="browser-default form-control"
              id="formGroupExampleInput"
              placeholder="Enter a number"
            />
            {% include "partials/fpp-fpb-details.liquid" %}
          </fieldset>
          <fieldset class="border mb-2 px-3 pb-3 pt-1">
            <legend>FPB (Pre-Project)</legend>
            <label for="formGroupExampleInput2" class="sr-only">FPB (Pre-Project)</label>
            <input
              type="text"
              inputmode="numeric"
              class="browser-default form-control"
              id="formGroupExampleInput2"
              placeholder="Enter a number"
            />
            {% include "partials/fpp-fpb-details.liquid" %}
          </fieldset>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-link text-dark" data-dismiss="modal">Cancel</button>
          <button type="button" class="btn btn-purple" data-dismiss="modal">
            Add
          </button>
        </div>
      </div>
    </div>
  </div>
{% endblock %}

{% block script %}
  <script defer>
    document.addEventListener("DOMContentLoaded", function () {
      // Use querySelector with escaped ID: \\5 is how you escape 5
      const select = document.querySelector("[id='530select']");

      function updateView() {
        const selected = select.value; // Get the selected option's value

        // Remove "active" class from all tables
        document.querySelectorAll(".table").forEach(el => {
          el.classList.remove("active");
        });

        // Add "active" class to the selected table
        document.getElementById(`table-${selected}`).classList.add("active");
      }

      // Set default view
      updateView();

      // Listen for changes in dropdown selection
      select.addEventListener("change", updateView);
    });

    document.addEventListener("DOMContentLoaded", function () {
      $('#modal-530').on('hidden.bs.modal', function () {
        // Close any open <details>
        this.querySelectorAll('details[open]').forEach(detail => {
          detail.removeAttribute('open');
        });

        // Clear inputs (text, checkbox, radio, etc.)
        this.querySelectorAll('input').forEach(input => {
          if (input.type === 'checkbox' || input.type === 'radio') {
            input.checked = false;
          } else {
            input.value = '';
          }
        });

        // Also clear textareas
        this.querySelectorAll('textarea').forEach(textarea => {
          textarea.value = '';
        });

        // Reset selects
        this.querySelectorAll('select').forEach(select => {
          select.selectedIndex = 0;
        });
      });
    });

  </script>

  </script>
{% endblock %}

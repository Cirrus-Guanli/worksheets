<header class="d-flex justify-content-between mb-2">
  <!-- Button trigger modal -->
  <button type="button" class="btn btn-sm btn-outline-secondary" data-toggle="modal" data-target="#exampleModal">
    + Add Project
  </button>
  <a href="#" class="btn btn-sm btn-outline-secondary">Discard</a>
</header>

<table class="table table-sm table-bordered table-hover worksheet-table mb-0">
  <caption class="sr-only">{{ title }}</caption>
  <thead>
    <tr>        
      <th scope="col" rowspan="2" class="bg-bg-blue text-center align-middle">Actions</th>
      <th scope="col" rowspan="2" class="bg-bg-blue text-center align-middle">ID</th>
      <th scope="col" rowspan="2" class="bg-bg-blue align-middle">Project Name</th>
      <th scope="col" rowspan="2" class="bg-bg-blue align-middle text-center">Points per Topic</th>
      <th scope="col" colspan="6" class="bg-bg-blue text-center align-middle">Topics Covered</th>
      <th scope="col" colspan="4" class="bg-bg-blue text-center align-middle">PPI Messages</th>
      <th scope="col" rowspan="2" class="bg-bg-blue align-middle text-center">Times per Year</th>
      <th scope="col" rowspan="2" class="bg-bg-blue align-middle text-center">Count</th>
      <th scope="col" rowspan="2" class="bg-bg-blue align-middle text-center">OP</th>
      <th scope="col" colspan="5" class="bg-bg-blue text-center align-middle">Multipliers</th>
    </tr>
    <tr>
      <th class="bg-bg-blue align-middle text-center" scope="col">Hazard</th>
      <th class="bg-bg-blue align-middle text-center" scope="col">Insure</th>
      <th class="bg-bg-blue align-middle text-center" scope="col">People</th>
      <th class="bg-bg-blue align-middle text-center" scope="col">Property</th>
      <th class="bg-bg-blue align-middle text-center" scope="col">Build</th>
      <th class="bg-bg-blue align-middle text-center" scope="col">Natural</th>
      <th scope="col" class="text-nowrap bg-bg-blue align-middle text-center">Msg 1</th>
      <th scope="col" class="text-nowrap bg-bg-blue align-middle text-center">Msg 2</th>
      <th scope="col" class="text-nowrap bg-bg-blue align-middle text-center">Msg 3</th>
      <th scope="col" class="text-nowrap bg-bg-blue align-middle text-center">Msg 4</th>
      <th scope="col" class="text-nowrap bg-bg-blue align-middle text-center">PPI?</th>
      <th scope="col" class="text-nowrap bg-bg-blue align-middle text-center">PPI (OP)</th>
      <th scope="col" class="text-nowrap bg-bg-blue align-middle text-center">STK?</th>
      <th scope="col" class="text-nowrap bg-bg-blue align-middle text-center">STK (OP)</th>
      <th scope="col" class="text-nowrap bg-bg-blue align-middle text-center">OP + PPI + STK</th>
    </tr>
  </thead>
  <tbody>
    {% for project in outreachprojects %}
      <tr>
        <td class="text-center align-middle text-text-ph"><span class="align-middle material-symbols-outlined">delete</span><span class="ml-2 align-middle material-symbols-outlined">edit</span></td>
        <th class="text-center align-middle" scope="row">OP#{{ forloop.index }}</th>
        <td class="text-nowrap align-middle">{{ project.name }}</td>
        <td class="text-center align-middle">{{ project.points_per_topic }}</td>
        <td class="text-center align-middle">{% if project.topic_hazard %}&#10003;{% endif %}</td>
        <td class="text-center align-middle">{% if project.topic_insure %}&#10003;{% endif %}</td>
        <td class="text-center align-middle">{% if project.topic_people %}&#10003;{% endif %}</td>
        <td class="text-center align-middle">{% if project.topic_property %}&#10003;{% endif %}</td>
        <td class="text-center align-middle">{% if project.topic_build %}&#10003;{% endif %}</td>
        <td class="text-center align-middle">{% if project.topic_natural %}&#10003;{% endif %}</td>
        <td class="text-center align-middle">{% if project.msg1 %}&#10003;{% endif %}</td>
        <td class="text-center align-middle">{% if project.msg2 %}&#10003;{% endif %}</td>
        <td class="text-center align-middle">{% if project.msg3 %}&#10003;{% endif %}</td>
        <td class="text-center align-middle">{% if project.msg4 %}&#10003;{% endif %}</td>
        <td class="text-center align-middle">{{ project.times_per_year }}</td>
        <td class="text-center align-middle">{{ project.count }}</td>
        <td class="text-center align-middle">{{ project.op }}</td>
        <td class="text-center align-middle">{{ project.ppi? }}</td>
        <td class="text-center align-middle">{{ project.ppi_op }}</td>
        <td class="text-center align-middle">{{ project.stk? }}</td>
        <td class="text-center align-middle">{{ project.stk_op }}</td>
        <td class="text-center align-middle">{{ project.op_ppi_stk }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>

{% block modal %}
  <!-- Modal -->
  <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-scrollable">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Add Project</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <fieldset class="border mb-2 px-3 pb-3 pt-1">
            <legend>Project Name</legend>
            <label for="formGroupExampleInput" class="sr-only">Project Name</label>
            <input type="text" class="form-control" id="formGroupExampleInput" placeholder="Enter a project name">
          </fieldset>
          <fieldset class="border mb-2 px-3 pb-3 pt-1">
            <legend>Points per Topic</legend>
            <label for="formGroupExampleInput2" class="sr-only">Points per Topic</label>
            <input type="text" inputmode="numeric" class="form-control" id="formGroupExampleInput2"
              placeholder="Enter a number">
          </fieldset>
          <fieldset class="border mb-2 px-3 pb-3 pt-1">
            <legend>Topics Covered</legend>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="checkbox" id="inlineCheckbox1" value="option1">
              <label class="form-check-label" for="inlineCheckbox1">Hazard</label>
            </div>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="option2">
              <label class="form-check-label" for="inlineCheckbox2">Insure</label>
            </div>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="checkbox" id="inlineCheckbox3" value="option3">
              <label class="form-check-label" for="inlineCheckbox3">People</label>
            </div>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="checkbox" id="inlineCheckbox4" value="option4">
              <label class="form-check-label" for="inlineCheckbox4">Property</label>
            </div>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="checkbox" id="inlineCheckbox5" value="option5">
              <label class="form-check-label" for="inlineCheckbox5">Build</label>
            </div>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="checkbox" id="inlineCheckbox6" value="option6">
              <label class="form-check-label" for="inlineCheckbox6">Natural</label>
            </div>
          </fieldset>
          <fieldset class="border mb-2 px-3 pb-3 pt-1">
            <legend>PPI Messages</legend>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="checkbox" id="inlineCheckbox7" value="option7">
              <label class="form-check-label" for="inlineCheckbox7">Msg 1</label>
            </div>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="checkbox" id="inlineCheckbox8" value="option8">
              <label class="form-check-label" for="inlineCheckbox8">Msg 2</label>
            </div>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="checkbox" id="inlineCheckbox9" value="option9">
              <label class="form-check-label" for="inlineCheckbox9">Msg 3</label>
            </div>
            <div class="form-check form-check-inline">
              <input class="form-check-input" type="checkbox" id="inlineCheckbox10" value="option10">
              <label class="form-check-label" for="inlineCheckbox10">Msg 4</label>
            </div>
          </fieldset>
          <fieldset class="border mb-2 px-3 pb-3 pt-1">
            <legend>Times per Year</legend>
            <label for="formGroupExampleInput" class="sr-only">Times per Year</label>
            <input type="text" inputmode="numeric" class="form-control" id="formGroupExampleInput" placeholder="Enter a number">
          </fieldset>
          <fieldset class="border mb-2 px-3 pb-3 pt-1">
            <legend>Count</legend>
            <label for="formGroupExampleInput2" class="sr-only">Count</label>

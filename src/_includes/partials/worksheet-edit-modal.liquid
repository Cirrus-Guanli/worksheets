
{% block modal %}
  <!-- Modal -->
  <div
    class="modal fade"
    id="exampleModal2"
    tabindex="-1"
    aria-labelledby="exampleModalLabel2"
    aria-hidden="true"
  >
    <div class="modal-dialog modal-dialog-scrollable">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">
            Edit {% if title == '530Credit â€¢ Flood Protection Credit Calculation Checklist Worksheet' %}Building{% else %}Project{% endif %}
          </h5>
          <button
            type="button"
            class="close"
            data-dismiss="modal"
            aria-label="Close"
          >
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <fieldset class="border mb-2 px-3 pb-3 pt-1">
            <legend>Example Field</legend>
            <label for="formGroupExampleInput" class="sr-only"
              >Example Field 1</label
            >
            <input
              type="text"
              class="form-control"
              id="formGroupExampleInput"
              placeholder="Example Field 1"
            />
          </fieldset>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn" data-dismiss="modal">Cancel</button>
          <button type="button" class="btn btn-purple" data-dismiss="modal">
            Save
          </button>
        </div>
      </div>
    </div>
  </div>
{% endblock %}
